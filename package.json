{
  "name": "karatefeaturetestgen",
  "displayName": "Karate Feature Test Generator",
  "description": "Automatically generate Karate Feature Tests from OpenAPI/Swagger specs or JSON files.",
  "version": "1.0.2",
  "publisher": "SureshNettur",
  "engines": {
    "vscode": "^1.36.1"
  },
  "icon": "icon.png",
  "author": "Suresh Nettur",
  "contributors": [
    "Akhil Dusi",
    "Unnati Nettur"
  ],
  "categories": [
    "Programming Languages", 
    "Snippets",
    "Formatters",
    "Other"
  ],
  "keywords": [
    "Json", 
    "Formatter", 
    "Validator", 
    "JSON tools",
    "QA",
    "Quality Assurance", 
    "Karate",
    "Feature",
    "generate",
    "generation",
    "java",
    "Test",
    "Tests",
    "Automatically",
    "Util",
    "Utility",
    "Code Generator",
    "Development",
    "Testing",
    "Tools",
    "Auto Generator"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "karatefeatureautogen.processTest",
        "title": "Karate Feature Test Generator"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "serve": "live-server src/index.html --verbose",
    "start": "npm run serve",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema-faker": "^0.5.4",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.36.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "live-server": "^1.2.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "config": "^3.3.12",
    "openai": "^4.55.7",
    "openapi-schema-validator": "^12.1.3",
    "swagger-client": "^3.29.4",
    "swagger-parser": "^10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nsuresh9806/tbd.git"
  }
}
